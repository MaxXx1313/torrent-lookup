    "command-line-usage": "^6.1.3",
    "ignore": "^5.0.4",
    "line-by-line": "^0.1.5",
    "minimatch": "^3.0.4",
    "rxjs": "^5.5.6",
    "single-line-log": "^1.1.2"
  },
  "devDependencies": {
    "@types/command-line-args": "^5.2.0",
    "@types/command-line-usage": "^5.0.2",
    "@types/node": "^16.18.12",
    "@types/single-line-log": "^1.1.0",
    "mocha": "^10.2.0",
    "typescript": "^4.9.5"
  }
